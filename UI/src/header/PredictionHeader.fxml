      <ColumnConstraints maxWidth="30.0" minWidth="0.0" prefWidth="0.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="35.0" minHeight="20.0" prefHeight="20.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="28.0" minHeight="28.0" prefHeight="28.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="5.0" minHeight="5.0" prefHeight="5.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="37.0" minHeight="37.0" prefHeight="37.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label fx:id="headLineLable" alignment="CENTER" prefHeight="66.0" prefWidth="146.0" text="Prediciotns" GridPane.columnIndex="6" />
      <Button fx:id="loadFileButton" mnemonicParsing="false" onAction="#loadFileButtonClicked" prefHeight="27.0" prefWidth="140.0" text="Load File" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Label fx:id="currentFileLabel" prefHeight="39.0" prefWidth="860.0" text="Currently Loaded File" GridPane.columnIndex="3" GridPane.columnSpan="7" GridPane.rowIndex="1">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Label>
      <Button fx:id="detailsButton" alignment="CENTER" mnemonicParsing="false" onAction="#detailsButtonClicked" prefHeight="30.0" prefWidth="163.0" styleClass="HeaderMenuButtons" text="Details" GridPane.columnIndex="4" GridPane.rowIndex="3">
